{
  "name": "frontend",
  "author": "TMT",
  "version": "0.1.0",
  "license": "Apache License, Version 2.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@tmtsoftware/esw-ts": "git+https://github.com/tmtsoftware/esw-ts#e6206a5f37205a8115b52da21db980e27be994c1",
    "antd": "^4.16.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "scripts": {
    "start": "snowpack dev",
    "build": "snowpack build",
    "test:unit": "web-test-runner 'test/**/*.unit.test.{ts,tsx}'",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test": "npm run test:unit",
    "clean": "rm -rf dist && mkdir dist",
    "prebuild": "npm-run-all --parallel clean eslint:check style:check audit",
    "eslint:check": "eslint --config .eslintrc.json './{src,test}/**/*.{ts,tsx}'",
    "fix": "npm-run-all --parallel clean fix:eslint fix:prettier style:check",
    "fix:eslint": "eslint --fix --config .eslintrc.json './**/*.{ts,tsx}'",
    "fix:prettier": "prettier --write --config ./.prettierrc './**/*.{ts,tsx}' ",
    "audit": "npm audit",
    "style:check": "stylelint 'src/**/*.css' "
  },
  "browserslist": [
    "last 1 chrome version"
  ],
  "devDependencies": {
    "@snowpack/plugin-typescript": "^1.2.1",
    "@snowpack/web-test-runner-plugin": "^0.2.2",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^7.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/chai": "^4.2.18",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "@web/dev-server-import-maps": "^0.0.5",
    "@web/test-runner": "^0.13.7",
    "chai": "^4.3.4",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^4.6.0",
    "keycloak-js": "^13.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "snowpack": "^3.5.6",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "ts-mockito": "^2.6.1",
    "typescript": "^4.3.2"
  }
}
